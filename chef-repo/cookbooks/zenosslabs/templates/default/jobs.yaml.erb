<% if @discovery_jobs %>
discovery:
    <% @discovery_jobs.each do |job| %>
    "<%= job[:name] %>":
        url: <%= job[:url] %>
        scm: <%= job[:scm] %>
        jenkins_tag: <%= job[:jenkins_tag] %>
        jenkins_filter: <%= job[:jenkins_filter] %>

    <% end %>
<% end %>

<% if @zenpack_jobs %>
zenpack:
    <% @zenpack_jobs.each do |job| %>
    "<%= job[:name] %>":
        url: <%= job[:url] %>
        scm: <%= job[:scm] %>
        jenkins_filter: <%= job[:jenkins_filter] %>

    <% end %>
<% end %>
